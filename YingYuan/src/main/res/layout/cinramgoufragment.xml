<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dp_24"
    android:layout_marginRight="@dimen/dp_24"
    android:layout_marginBottom="@dimen/dp_20"
    android:background="@drawable/backgroup">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cineam_gou_name"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_10"
        android:textColor="#999"
        android:textSize="@dimen/sp_14"
        android:text="啦啦啦啦"/>
       <TextView
           android:id="@+id/cineam_gou_yuyan"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginTop="@dimen/dp_10"
           android:textSize="@dimen/sp_14"
           android:text="国语3D"
           android:layout_marginLeft="@dimen/dp_20"
           android:layout_toRightOf="@+id/cineam_gou_name"/>
    <TextView
        android:id="@+id/cineam_gou_staTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:textSize="@dimen/sp_24"
        android:text="啦啦啦啦"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_below="@+id/cineam_gou_name"/>
    <TextView
        android:id="@+id/www"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_15"
        android:textSize="@dimen/sp_14"
        android:text="——"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_below="@+id/cineam_gou_name"
        android:layout_toRightOf="@+id/cineam_gou_staTime"/>
    <TextView
    android:id="@+id/endtime"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/dp_15"
    android:textSize="@dimen/sp_18"
    android:text="哈哈哈"
    android:layout_marginLeft="@dimen/dp_5"
    android:layout_below="@+id/cineam_gou_name"
    android:layout_toRightOf="@+id/www"/>
    <TextView
        android:id="@+id/end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:textSize="@dimen/sp_12"
        android:text="end"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_below="@+id/cineam_gou_name"
        android:layout_toRightOf="@+id/endtime"/>
    <TextView
        android:id="@+id/jia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_30"
        android:textSize="@dimen/sp_18"
        android:text="￥"
        android:layout_marginLeft="@dimen/dp_10"

        android:layout_toRightOf="@+id/end"/>
    <TextView
        android:id="@+id/cineam_gou_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:textSize="@dimen/sp_30"
        android:text="￥"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_toRightOf="@+id/jia"/>
  <ImageView
      android:layout_width="@dimen/dp_24"
      android:layout_height="@dimen/dp_24"
      android:src="@mipmap/com_icon_next_default_xhdpi"
      android:layout_marginTop="@dimen/dp_20"
      android:textSize="@dimen/sp_30"
      android:text="￥"
      android:layout_marginLeft="@dimen/dp_5"
      android:layout_toRightOf="@+id/price"
      />

</RelativeLayout>